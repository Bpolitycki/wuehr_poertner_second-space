{
  "name": "wuehr_poertner_second-space",
  "version": "0.0.0",
  "scripts": {
    "convert": "cd ./src/assets/data && python convert.py && cd ../../..",
    "dev": "pnpm run convert && vite",
    "build": "run-p type-check build-only",
    "build_app": "npx cap sync && npx cap open ios",
    "app": "pnpm run build && pnpm run build_app",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "bulma": "^0.9.4",
    "bulma-checkbox": "^1.2.1",
    "bulma-checkradio": "^2.1.3",
    "d3-selection": "^3.0.0",
    "d3-zoom": "^3.0.0",
    "fuse.js": "^6.6.2",
    "i": "^0.3.7",
    "npm": "^9.1.2",
    "pinia": "^2.0.20",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@capacitor/cli": "^3.7.0",
    "@capacitor/core": "^3.7.0",
    "@capacitor/ios": "^3.7.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/d3-selection": "^3.0.3",
    "@types/d3-zoom": "^3.0.1",
    "@types/jsdom": "^16.2.15",
    "@types/node": "^16.11.52",
    "@vitejs/plugin-vue": "^2.3.4",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^8.7.1",
    "jsdom": "^19.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sass": "^1.54.5",
    "typescript": "~4.7.4",
    "vite": "^2.9.15",
    "vitest": "^0.13.1",
    "vue-tsc": "^0.35.2"
  }
}
